
    <%= javascript_include_tag 'select2', :plugin=> 'redmine_project_cf_autocomplete' %>
    <%= stylesheet_link_tag 'select2', :plugin=> 'redmine_project_cf_autocomplete' %>
    <%= stylesheet_link_tag 'bootstrap', :plugin=> 'redmine_project_cf_autocomplete' %>

   <script type="text/javascript">
       $(document).ready(function(){

        //<![CDATA[
            <% @hash.each do |key, value|   %>
               tab = [];
               <% value.each do |v|%>
                     tab.push("<%= v %>");
               <% end%>
               var availableTags<%=key%> = tab;
                $("#project_custom_field_values_<%= key %>").css("borderColor","#00ACEE");
                $("#project_custom_field_values_<%= key %>").autocomplete({
                    source: availableTags<%=key%>
                });
            <% end %>

        //]]>
        });
   </script>

